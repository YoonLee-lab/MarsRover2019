<launch>
	<group ns="basestation">
	
		<node name="nmea_navsat_driver" pkg="nmea_navsat_driver" type="nmea_serial_driver" output="screen" respawn="true">
			<param name="baud" value="4800"/>
			<param name="port" value="/dev/basestation-gps"/> 
		</node>
		<node name="antenna" pkg="antenna" type="antenna_node" output="screen" respawn="true"/>
	</group>
</launch>
